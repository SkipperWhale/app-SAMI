                            <script>
                                function myFunction() {
                                  var txt;
                                  if (confirm("Invia Molleggi")) {
                                    txt = "Molleggi inviati oggi ";
                                  } else {
                                    txt = "Non hai inviato i molleggi!";
                                  }
                                  document.getElementById("demo").innerHTML = txt;
                                }
                                </script>
                                
						</div>
